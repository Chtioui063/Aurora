6;\n    letter-spacing:.08em;\n  }\n</style>\n<script>\n(function(){\n  const KEY = \"pillowa_countdown_end\";\n  const DEFAULT_SECONDS = 20 * 60; // 20 minutes\n  const d = document.getElementById(\"p-days\");\n  const h = document.getElementById(\"p-hours\");\n  const m = document.getElementById(\"p-minutes\");\n  const s = document.getElementById(\"p-seconds\");\n  if(!d||!h||!m||!s) return;\n  let endTs = parseInt(localStorage.getItem(KEY), 10);\n  const now = Math.floor(Date.now()/1000);\n  if(!endTs || endTs <= now){\n    endTs = now + DEFAULT_SECONDS;\n    localStorage.setItem(KEY, String(endTs));\n  }\n  const tick = () => {\n    const now = Math.floor(Date.now()/1000);\n    let remaining = Math.max(0, endTs - now);\n    const days = Math.floor(remaining / 86400);\n    remaining %= 86400;\n    const hours = Math.floor(remaining / 3600);\n    remaining %= 3600;\n    const minutes = Math.floor(remaining / 60);\n    const seconds = remaining % 60;\n    d.textContent = String(days).padStart(2,\"0\");\n    h.textContent = String(hours).padStart(2,\"0\");\n    m.textContent = String(minutes).padStart(2,\"0\");\n    s.textContent = String(seconds).padStart(2,\"0\");\n    if(endTs - now <= 0){\n      clearInterval(iv);\n    }\n  };\n  let iv = setInterval(tick, 1000);\n  tick();\n})();\n</script>\n<!-- FIN COMPTEUR -->"
